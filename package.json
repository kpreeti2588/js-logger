{
  "name": "@kpreeti2588/js-logger",
  "version": "1.0.0",
  "description": "Tracing and Logging",
  "author": "Preeti Kumari <kpreeti2588@gmail.com>",
  "license": "UNLICENSED",
  "main": "dist/tracing.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kpreeti2588/js-logger"
  },
  "bugs": "https://github.com/kpreeti2588/js-logger",
  "dependencies": {
    "@nestjs/common": "^8.0.5",
    "@nestjs/core": "^8.0.5",
    "@nestjs/platform-express": "^8.0.5",
    "@nestjs/swagger": "^5.0.9",
    "@opentelemetry/auto-instrumentations-node": "^0.24.0",
    "@opentelemetry/core": "^0.24.0",
    "@opentelemetry/exporter-jaeger": "^0.24.0",
    "@opentelemetry/exporter-prometheus": "^0.24.0",
    "@opentelemetry/host-metrics": "^0.24.0",
    "@opentelemetry/metrics": "^0.24.0",
    "@opentelemetry/propagator-b3": "^0.24.0",
    "@opentelemetry/propagator-jaeger": "^0.24.0",
    "@opentelemetry/sdk-node": "^0.24.0",
    "@opentelemetry/tracing": "^0.24.0",
    "nestjs-otel": "^2.1.0",
    "nestjs-pino": "^1.4.0",
    "opentelemetry-node-metrics": "^1.0.2",
    "pino": "^6.13.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.5",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.10",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.2.4",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
